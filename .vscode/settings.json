{
  "cSpell.words": ["Bunq", "Bunqy", "openapi"],
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "never",
    "source.extension": "never"
  },
  "deno.enable": false,
  "editor.formatOnSave": true,
  "files.autoSave": "off",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2
}
